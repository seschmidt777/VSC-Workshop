// Aufgabe 4. Der Code enthält Fehler, löse diese und erhalte den vierten Buchstaben.

def addiere(a, b):
    return a + b

def subtrahiere(a, b):
    return a - b

def multipliziere(a, b)
    return a * b  # Fehler 1: Fehlender Doppelpunkt am Ende der Funktionsdefinition

def dividiere(a, b):
    if b = 0:  # Fehler 2: Zuweisungsoperator statt Vergleichsoperator '=='
        raise ValueError("Division durch Null ist nicht erlaubt!")
    return a / b

def berechne_buchstabe():
    ascii_code = 64 + 9
    return chr(ascii_code)

def hauptfunktion():
    x = 10
    y = 0

    addition = addiere(x, y)
    print(f"Addition: {addition}")

    subtraktion = subtrahiere(x, y)
    print(f"Subtraktion: {subtraktion}")

    multiplikation = multipliziere(x, y)
    print(f"Multiplikation: {multiplikation}")

    try:
        division = dividiere(x, y)
        print(f"Division: {division}")
    except ValueError as fehler:
        print(feler)  

    buchstabe = berechne_buchstabe()
    print("Nächster Hinweis: " + buchstabe)

    text = "Ergebnis: " + 12345  
    print(text)

    liste = [1, 2, 3]
    print(liste[3])  

if __name__ == "__main__":
    hauptfunktion()
